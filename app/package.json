{
  "name": "randomgen",
  "version": "1.0.0",
  "description": "Random generators for RPGs",
  "keywords": [
    "rpg",
    "random",
    "generators"
  ],
  "author": "Marcin Forseti PaÅºdziora",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forsetius/randomgen-nest2.git"
  },
  "private": true,
  "engines": {
    "node": ">=22.11.0"
  },
  "main": "dist/main.js",
  "scripts": {
    "prepare": "cd .. && husky app/.husky",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --report-unused-disable-directives --max-warnings 0 --fix",
    "test": "npm run tsc && jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm run tsc && jest --config ./jest-e2e.config.json --runInBand --detectOpenHandles",
    "tsc": "tsc"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/swagger": "^8.1.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^6.3.0",
    "class-transformer": "^0.5.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.5",
    "helmet": "^8.0.0",
    "luxon": "^3.5.0",
    "marked": "^15.0.6",
    "nunjucks": "^3.2.4",
    "reflect-metadata": "0.1.14",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.15",
    "@tsconfig/node22": "^22.0.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.10.5",
    "@types/nunjucks": "^3.2.6",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "eslint --report-unused-disable-directives --max-warnings 0",
      "prettier --ignore-unknown --write"
    ]
  }
}
